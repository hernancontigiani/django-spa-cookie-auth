server {
	listen 80;
	listen [::]:80;

	# SSL configuration
    listen 443 ssl;
	listen [::]:443 ssl;
    ssl_certificate /etc/letsencrypt/live/inovecode.com/fullchain.pem;
    ssl_certificate_key /etc/letsencrypt/live/inovecode.com/privkey.pem;

	server_name campus.inovecode.com;
    
    location / {
        root /home/django-spa-cookie-auth/django_react_drf_cross_origin/frontend/dist;
        #return 301 /admin;
        # First attempt to serve request as file, then
        # as directory, then fall back to displaying a 404.
        try_files $uri $uri/ /index.html =404;
    }
}